public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
if (ispassenger)
{
if (GetVehicleModel(vehicleid) == 570 || GetVehicleModel(vehicleid) == 538)
{
IsInTren[playerid]=vehicleid;
SetPlayerPos(playerid, 1534.9965,1539.1204,10.8701);
SetPlayerFacingAngle(playerid, 0);
SetCameraBehindPlayer(playerid);
SetPlayerInterior(playerid, 5);
}
}
return 1;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
return 1;
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
if (newkeys == 16)
{
if(IsInTren[playerid] > 0)
{
new Float:X,Float:Y,Float:Z;
GetVehiclePos(IsInTren[playerid], X, Y, Z);
SetPlayerPos(playerid, X-15, Y, Z);
SetPlayerInterior(playerid, 0);
IsInTren[playerid]=0;
}
}
return 1;
}

zcmd(irtren,playerid, params[]);
{
if(IsPlayerAdmin(playerid))
{
SetPlayerPos(playerid, 1534.9965,1539.1204,10.8701);
SetPlayerFacingAngle(playerid, 0);
SetCameraBehindPlayer(playerid);
SetPlayerInterior(playerid, 1);
}
return 1;
}
return 0;
}

public OnPlayerDeath(playerid, killerid, reason)
{
IsInTren[playerid] = 0;
return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
IsInTren[playerid] = 0;
return 1;
}

public OnVehicleDeath(vehicleid)
{
if(GetVehicleModel(vehicleid) == 538)
{
for(new i=0; i<MAX_PLAYERS; i++)
{
if(IsInTren[i] != 0)
{
SetPlayerHealth(i, 0);
SendClientMessage(i, COLOR_RED, "El Tren Se Estrello.");
}
}
}
return 1;
}
